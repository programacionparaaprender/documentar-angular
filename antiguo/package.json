{
  "name": "starter-bootstrap-admin-angular6",
  "version": "0.2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4600 --open",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start2": "ng serve",
    "prod": "ng build --prod --base-href='./'",
    "firebase": "ng build --prod && firebase deploy",
    "compodoc": "npx compodoc -p src/tsconfig.app.json --output src/docs --serve --open",
    "compodoc-material": "npx compodoc -p src/tsconfig.app.json --output src/docs --serve --open --theme material",
    "compodoc-laravel": "npx compodoc -p src/tsconfig.app.json --output src/docs --serve --open --theme laravel",
    "compodoc-vagrant": "npx compodoc -p src/tsconfig.app.json --output src/docs --serve --open --theme vagrant",
    "compodoc-plus": "npx compodoc -p src/tsconfig.app.json --output src/docs --serve --open --includes src/aditional-docs",
    "compodoc-favicon-title": "npx compodoc -p src/tsconfig.app.json --output src/docs --serve --open --includes src/aditional-docs --name \"Angular Compodoc Cource\" --customFavicon src/favicon.ico  ",
    "compodoc-custom-style": "npx compodoc -p src/tsconfig.app.json --output src/docs --serve --open --includes src/aditional-docs --name \"Angular Compodoc Cource\" --customFavicon src/favicon.ico -y ./css/",
    "compodoc-favicon-title-personal": "npx compodoc -p src/tsconfig.app.json --output src/docs --serve --open --includes src/aditional-docs --name \"Angular Compodoc Cource\" --customFavicon src/favicon.ico --hideGenerator ",
    "compodoc-custom-style-personal": "npx compodoc -p src/tsconfig.app.json --output src/docs --serve --open --includes src/aditional-docs --name \"Angular Compodoc Cource\" --customFavicon src/favicon.ico -y ./css/ --hideGenerator "
  },
  "private": true,
  "dependencies": {
    "@agm/core": "1.0.0-beta.3",
    "@agm/js-marker-clusterer": "1.0.0-beta.3",
    "@angular/animations": "6.0.3",
    "@angular/common": "~10.0.0",
    "@angular/compiler": "6.0.3",
    "@angular/core": "~10.0.0",
    "@angular/forms": "~10.0.0",
    "@angular/http": "6.0.3",
    "@angular/platform-browser": "~10.0.0",
    "@angular/platform-browser-dynamic": "~10.0.0",
    "@angular/router": "~10.0.0",
    "@toverux/ngx-sweetalert2": "4.0.0",
    "chart.js": "2.7.3",
    "core-js": "2.5.4",
    "i18n-config": "1.0.4",
    "js-marker-clusterer": "1.0.0",
    "ng2-charts": "1.6.0",
    "ngx-easy-table": "5.1.0",
    "rxjs": "^6.6.2",
    "sweetalert2": "7.25.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@angular-devkit/build-angular": "~0.1000.0",
    "@angular/cli": "~10.0.5",
    "@angular/compiler": "~10.0.0",
    "@angular/compiler-cli": "~10.0.0",
    "@angular/language-service": "~10.0.0",
    "@ionic/angular-toolkit": "^2.3.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  }
}
